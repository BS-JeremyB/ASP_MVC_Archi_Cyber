@{

    // Fruit fruit = JsonSerializer.Deserialize<Fruit>(TempData["fruit"].ToString());
    Fruit fruit = new Fruit
            {
                Id = (int)TempData["Id"],
                Name = TempData["Name"].ToString(),
                Origin = TempData["Origin"].ToString(),
                IsBio = (bool)TempData["IsBio"]
            };


}
<h1>@fruit.Name</h1>
<ul>
    <li>Id : @fruit.Id</li>
    <li>Origine : @fruit.Origin</li>
    <li>Bio : @fruit.IsBio</li>
</ul>